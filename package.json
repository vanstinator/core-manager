{
  "name": "core-manager",
  "version": "1.2.0",
  "author": "Justin Vanderhooft",
  "main": "serve/main.js",
  "description": "A libretro game emulator core manager for Plex Media Server",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "npm-run-all -p electron:serve ng:serve",
    "build": "npm run electron:serve-tsc && ng build --base-href ./",
    "build:dev": "npm run build -- -c dev",
    "build:prod": "npm run build -- -c production",
    "ng:serve": "ng serve -c web -o --port 6001",
    "electron:serve-tsc": "tsc -p tsconfig.serve.json",
    "electron:serve": "npm run electron:serve-tsc && electron . --serve",
    "electron:local": "npm run build:prod && npx electron .",
    "electron:build": "dotenv npm run build:prod && dotenv electron-forge make && node ./bin/cleanReleases.js",
    "test": "ng test --watch=false",
    "test:watch": "ng test",
    "e2e": "npm run build:prod && cross-env TS_NODE_PROJECT='e2e/tsconfig.e2e.json' mocha --timeout 300000 --require ts-node/register e2e/**/*.e2e.ts",
    "version": "conventional-changelog -i CHANGELOG.md -s -r 0 && git add CHANGELOG.md",
    "lint": "ng lint",
    "release:patch": "dotenv release-it -- patch --ci",
    "release:minor": "dotenv release-it -- minor --ci",
    "release": "dotenv release-it --ci",
    "package": "dotenv electron-forge package",
    "make": "dotenv electron-forge make"
  },
  "dependencies": {
    "custom-electron-titlebar": "3.2.6",
    "electron-log": "4.3.1",
    "electron-squirrel-startup": "^1.0.0",
    "electron-store": "7.0.0",
    "got": "9.6.0",
    "keytar": "7.3.0",
    "unzipper": "0.10.11",
    "xml2js": "0.4.23",
    "xmlbuilder2": "2.4.0"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "10.0.1",
    "@angular-devkit/build-angular": "0.1100.3",
    "@angular-eslint/builder": "0.8.0-beta.3",
    "@angular-eslint/eslint-plugin": "0.8.0-beta.3",
    "@angular-eslint/eslint-plugin-template": "0.8.0-beta.3",
    "@angular-eslint/schematics": "0.8.0-beta.3",
    "@angular-eslint/template-parser": "0.8.0-beta.3",
    "@angular/animations": "^11.0.0 || ^12.0.0-0",
    "@angular/cdk": "11.1.1",
    "@angular/cli": "11.0.3",
    "@angular/common": "11.0.3",
    "@angular/compiler": "11.0.3",
    "@angular/compiler-cli": "11.0.3",
    "@angular/core": "11.0.3",
    "@angular/forms": "11.0.3",
    "@angular/language-service": "11.0.3",
    "@angular/localize": "11.0.3",
    "@angular/material": "11.1.1",
    "@angular/platform-browser": "11.0.3",
    "@angular/platform-browser-dynamic": "11.0.3",
    "@angular/router": "11.0.3",
    "@electron-forge/cli": "^6.0.0-beta.54",
    "@electron-forge/maker-deb": "^6.0.0-beta.54",
    "@electron-forge/maker-dmg": "6.0.0-beta.54",
    "@electron-forge/maker-rpm": "^6.0.0-beta.54",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.54",
    "@electron-forge/maker-zip": "^6.0.0-beta.54",
    "@ngx-translate/core": "13.0.0",
    "@ngx-translate/http-loader": "6.0.0",
    "@release-it/conventional-changelog": "^2.0.0",
    "@types/jasmine": "3.6.2",
    "@types/jasminewd2": "2.0.8",
    "@types/mocha": "8.0.4",
    "@types/node": "12.12.6",
    "@typescript-eslint/eslint-plugin": "4.7.0",
    "@typescript-eslint/eslint-plugin-tslint": "4.7.0",
    "@typescript-eslint/parser": "4.7.0",
    "chai": "4.2.0",
    "conventional-changelog-cli": "2.1.1",
    "core-js": "3.6.5",
    "cross-env": "7.0.2",
    "dotenv": "8.2.0",
    "dotenv-cli": "^4.0.0",
    "electron": "11.2.0",
    "electron-reload": "1.5.0",
    "eslint": "7.13.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsdoc": "30.7.8",
    "eslint-plugin-prefer-arrow": "1.2.2",
    "eslint-plugin-simple-import-sort": "7.0.0",
    "jasmine-core": "3.6.0",
    "jasmine-spec-reporter": "6.0.0",
    "karma": "~5.1.0",
    "karma-coverage-istanbul-reporter": "3.0.3",
    "karma-electron": "6.3.1",
    "karma-jasmine": "4.0.1",
    "karma-jasmine-html-reporter": "1.5.4",
    "mocha": "8.2.1",
    "ngx-scrollbar": "7.5.4",
    "npm-run-all": "4.1.5",
    "release-it": "^14.2.2",
    "rxjs": "6.6.3",
    "spectron": "13.0.0",
    "ts-node": "9.1.0",
    "tslib": "2.0.3",
    "typescript": "4.0.5",
    "wait-on": "5.0.1",
    "webdriver-manager": "12.1.7",
    "zone.js": "0.10.3"
  },
  "volta": {
    "node": "15.6.0",
    "npm": "6.14.11"
  },
  "config": {
    "forge": "./config.forge.js"
  },
  "plugins": {
    "@release-it/conventional-changelog": {
      "preset": "conventionalcommits",
      "infile": "CHANGELOG.md"
    }
  }
}
